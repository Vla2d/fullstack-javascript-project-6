{
   "name": "@hexlet/code",
   "version": "1.0.0",
   "main": "src/server.js",
   "scripts": {
      "test": "jest",
      "lint-fix": "eslint --fix .",
      "build": "knex --esm migrate:latest & webpack",
      "start": "fastify start src/server.js -l trace -P -o",
      "create-db-migration": "knex --esm migrate:make $npm_config_name",
      "db-migrate": "knex --esm migrate:latest",
      "start-frontend": "webpack --watch --progress"
   },
   "jest": {
      "modulePathIgnorePatterns": [
         "<rootDir>/__tests__/helpers/"
      ],
      "setupFiles": [
         "dotenv/config"
      ]
   },
   "type": "module",
   "dependencies": {
      "@fastify/formbody": "^7.0.1",
      "@fastify/passport": "^2.2.0",
      "@fastify/secure-session": "^5.2.0",
      "@fastify/sensible": "^5.1.1",
      "@fastify/static": "^6.5.0",
      "@fastify/view": "^7.1.0",
      "bootstrap": "^5.2.1",
      "css-loader": "^6.7.1",
      "fastify": "^4.10.2",
      "fastify-cli": "^5.6.0",
      "fastify-method-override": "^1.5.9",
      "fastify-objectionjs": "^1.0.0",
      "fastify-reverse-routes": "^3.0.0",
      "i18next": "^21.9.1",
      "lodash": "^4.17.21",
      "objection": "^3.0.1",
      "objection-unique": "^1.2.2",
      "passport-local": "^1.0.0",
      "pg": "^8.8.0",
      "pino-pretty": "^9.1.1",
      "pug": "^3.0.2",
      "qs": "^6.11.0",
      "rollbar": "^2.25.2",
      "sqlite3": "^5.0.11",
      "style-loader": "^3.3.1",
      "webpack": "^5.74.0"
   },
   "devDependencies": {
      "@jest/globals": "^29.3.1",
      "eslint": "^8.23.0",
      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-jest": "^27.1.3",
      "jest": "^29.3.1",
      "webpack-cli": "^4.10.0"
   }
}
